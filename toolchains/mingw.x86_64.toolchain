[binaries]
c = '/opt/homebrew/bin/x86_64-w64-mingw32-gcc'
cpp = '/opt/homebrew/bin/x86_64-w64-mingw32-g++'
ar = '/opt/homebrew/bin/x86_64-w64-mingw32-ar'
strip = '/opt/homebrew/bin/x86_64-w64-mingw32-strip'
pkgconfig = '/opt/homebrew/bin/x86_64-w64-mingw32-pkg-config'

[host_machine]
system = 'windows'
cpu_family = 'x86_64'
cpu = 'amd64'
endian = 'little'

[built-in options]
c_args = ['-D_WIN32_WINNT=0x0601']
cpp_args = ['-D_WIN32_WINNT=0x0601']